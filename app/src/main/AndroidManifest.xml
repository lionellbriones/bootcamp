<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nonel.bootcamp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RatingActivity"
            android:label="@string/title_activity_rating" >
        </activity>
        <activity
            android:name=".CalculatorActivity"
            android:label="@string/title_activity_calculator" >
        </activity>
        <activity
            android:name=".SlamBookActivity"
            android:label="@string/title_activity_slam_book" >
        </activity>
        <activity
            android:name=".DeliveryActivity"
            android:label="@string/title_activity_delivery" >
        </activity>
        <activity
            android:name=".DeliveryFragmentActivity"
            android:label="@string/title_activity_delivery_fragment" >
        </activity>
    </application>

</manifest>
